<nav>
    Albums for {{items.0.artists.0.name}}
</nav>
<div class="cards">
    {{#each items}}
        <div class="cardItem">
            <strong>{{name}}</strong>
            <img src={{images.0.url}} />
            <button><a href='/tracks/{{id}}/'>View tracks</a></button>
        </div>
    {{/each}}
</div>
